{
  "name": "smart-services",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watch",
    "test:cov": "react-scripts test --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/react-scripts test --runInBand",
    "test:e2e": "cypress open"
  },
  "dependencies": {
    "js-cookie": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-oauth2-code-pkce": "^1.20.2",
    "react-router-dom": "^6.26.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "testMatch": [
      "**/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "**/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "postcss": "^8.4.45",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.1"
  }
}
